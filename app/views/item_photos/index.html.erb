<h1>Listing item_photos</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Is primary</th>
    <th>Photo file name</th>
    <th>Photo file content</th>
    <th>Photo file size</th>
    <th>Caption</th>
  </tr>

<% @item_photos.each do |item_photos| %>
  <tr>
    <td><%=h item_photos.item_id %></td>
    <td><%=h item_photos.is_primary %></td>
    <td><%=h item_photos.photo_file_name %></td>
    <td><%=h item_photos.photo_file_content %></td>
    <td><%=h item_photos.photo_file_size %></td>
    <td><%=h item_photos.caption %></td>
    <td><%= link_to 'Show', item_photos %></td>
    <td><%= link_to 'Edit', edit_item_photos_path(item_photos) %></td>
    <td><%= link_to 'Destroy', item_photos, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New item_photos', new_item_photos_path %>